    "800e7944-09c3-4cf7-9b18-4879c509b768": {
      "type": "image-with-text",
      "blocks": {
        "template--18984247460184__800e7944-09c3-4cf7-9b18-4879c509b768-1684558986517d353c-0": {
          "type": "heading",
